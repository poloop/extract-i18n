{
  "name": "extract-i18n",
  "version": "0.0.3",
  "description": "Script to extract missing or duplicate i18n keys in a project",
  "bin": {
    "extract-i18n": "./bin/extract-i18n.js"
  },
  "source": "./src/extract-i18n.ts",
  "main": "./dist/extract-i18n.js",
  "module": "./dist/extract-i18n.umd.js",
  "exports": "./dist/extract-i18n.modern.js",
  "scripts": {
    "build": "rm -fr ./dist && microbundle --target node -f umd,modern",
    "dev": "microbundle watch --target node -f umd,modern",
    "postinstall": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "javascript",
    "typescript",
    "i18n",
    "vue"
  ],
  "author": "plong",
  "license": "MIT",
  "dependencies": {
    "btoa": "^1.2.1",
    "commander": "^8.2.0",
    "dot-object": "^2.1.4",
    "glob": "^7.2.0",
    "is-valid-glob": "^1.0.0",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "terminal-kit": "^2.3.0"
  },
  "devDependencies": {
    "@types/dot-object": "^2.1.2",
    "@types/glob": "^7.1.4",
    "@types/is-valid-glob": "^1.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "jest": "^27.3.1",
    "microbundle": "^0.13.3",
    "ts-jest": "^27.0.7"
  }
}
